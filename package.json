{
    "name": "cheese-vault",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "start": "node dist/index.js",
        "build": "tsc",
        "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node -r tsconfig-paths/register' src/index.ts",
        "studio": "bunx drizzle-kit studio --config src/drizzle.config.ts",
        "generate": "bunx drizzle-kit generate --config src/drizzle.config.ts",
        "migrate": "bunx drizzle-kit migrate --config src/drizzle.config.ts",
        "push": "bunx drizzle-kit push --config src/drizzle.config.ts",
        "test": "jest",
        "test:watch": "jest --watch"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
    "devDependencies": {
        "@sqlite.org/sqlite-wasm": "^3.50.1-build1",
        "@types/node": "^22.15.30",
        "better-sqlite3": "^12.2.0",
        "drizzle-kit": "^0.31.1",
        "nodemon": "^3.1.10",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "tsx": "^4.20.3",
        "typescript": "^5.8.3"
    },
    "dependencies": {
        "@libsql/client": "^0.15.9",
        "@types/better-sqlite3": "^7.6.13",
        "@types/jest": "^30.0.0",
        "dotenv": "^16.5.0",
        "drizzle-orm": "^0.44.2",
        "jest": "^30.0.4",
        "telegraf": "^4.16.3",
        "ts-jest": "^29.4.0",
        "winston": "^3.17.0",
        "winston-daily-rotate-file": "^5.0.0"
    }
}
